{
  "name": "2048-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/scss/app.scss src/css/app.scss",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o src/css --watch --recursive --source-map true --source-map-contents sass"
  },
  "devDependencies": {
    "@types/jest": "^24.0.5",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "typescript": "^3.3.3"
  }
}
